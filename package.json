{
  "name": "@flexverse/repo-manager",
  "description": "Tools for managing repositories",
  "keywords": [
    "repository",
    "tools",
    "repo-manager",
    "manager",
    "repo",
    "cli",
    "command-line",
    "commands",
    "git",
    "lerna",
    "yarn workspaces",
    "mono-repo",
    "utils",
    "utility"
  ],
  "version": "1.5.2",
  "author": "Amir Mohsen Abdolrazaghi <amirmohsen.a@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "reman": "out/dist/js/bin.js"
  },
  "main": "out/dist/js/index.js",
  "types": "out/dist/dts/index.d.ts",
  "scripts": {
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "clean": "gulp clean",
    "build": "gulp build",
    "watch": "gulp watch",
    "dev": "node ./out/dist/js/bin",
    "types": "tsc --noEmit",
    "test": "jest --testLocationInResults --coverage",
    "test:watch": "jest --watch",
    "pr:before": "yarn && yarn format",
    "pr:checks": "yarn --frozen-lockfile && yarn types && yarn lint && yarn build && yarn test",
    "release": "semantic-release",
    "release:dry": "yarn release --dry-run --branches main,$(git branch --show-current)",
    "sync-versions": "babel-node --extensions \".ts\" build/updateRootPackageJSONVersion",
    "build:docs": "babel-node --extensions \".ts\" build/generateAPIReference",
    "format:docs": "prettier --write docs/api-reference.md CHANGELOG.md"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@types/fs-extra": "^9.0.8",
    "@types/jest": "^26.0.22",
    "@types/lodash.isempty": "^4.4.6",
    "@types/yargs": "^16.0.0",
    "babel-jest": "^26.6.3",
    "conventional-changelog-conventionalcommits": "^4.5.0",
    "del": "^6.0.0",
    "fs-extra": "^9.1.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-json-transform": "^0.4.7",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "jest": "^26.6.3",
    "jest-chain": "^1.1.5",
    "jest-github-reporter": "^1.0.2",
    "jest-mock-process": "^1.4.0",
    "json2md": "^1.10.0",
    "prettier": "^2.2.1",
    "ramda": "^0.27.1",
    "semantic-release": "^17.4.2",
    "strip-ansi": "^6.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@commitlint/lint": "^12.1.1",
    "@commitlint/load": "^12.1.1",
    "chalk": "^4.1.0",
    "enquirer": "^2.3.6",
    "lodash.isempty": "^4.4.0",
    "read-pkg-up": "^7.0.1",
    "simple-git": "^2.37.0",
    "yargs": "^16.2.0"
  }
}
